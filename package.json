{
  "name": "@tombatossals/react-chords",
  "description": "React library to easily render instrument chord diagrams",
  "version": "0.3.0",
  "private": false,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "prettier": "prettier --single-quote --write \"src/**/*.{js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tombatossals/react-chords.git"
  },
  "keywords": [
    "svg",
    "react",
    "javascript",
    "chords",
    "guitar",
    "ukelele"
  ],
  "author": "David Rubert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tombatossals/react-chords/issues"
  },
  "homepage": "https://tombatossals.github.io/react-chords",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
